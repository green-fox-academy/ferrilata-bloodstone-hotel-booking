@model RoomViewModel
@{
    ViewData["Title"] = "Reservation";
}

<h2>New Reservation</h2>

<form asp-controller="Rooms" asp-action="NewReservation" method="post">
    <button type="submit" class="btn btn-primary">Save</button>
    <a class="btn btn-danger" role="button" 
       asp-controller="Hotels" asp-action="Hotel" 
       asp-route-id="@Model.HotelId">Cancel</a>
</form>


